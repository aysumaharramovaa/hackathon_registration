{
  "version": 2,
  "builds": [
    { "src": "api/register.js", "use": "@vercel/node" },
    { "src": "api/applications.js", "use": "@vercel/node" },
    { "src": "public/**/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/register", "dest": "api/register.js" },
    { "src": "/api/applications", "dest": "api/applications.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
